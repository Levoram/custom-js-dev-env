{
  "name": "custom-javascript-development-environment",
  "version": "1.0.2",
  "description": "Javascript Development Environment: Setup Development faster",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel serve:src lint:watch test:watch start-mockapi",
    "serve:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "npm audit",
    "share": "lt --port 3000",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3005",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "author": "Nico Kuechler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Levoram/custom-js-dev-env"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "7.0.0",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.6",
    "caniuse-lite": "^1.0.30001240",
    "chai": "^4.3.4",
    "chalk": "^4.1.1",
    "cheerio": "*",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-watch": "^6.0.1",
    "express": "4.17.3",
    "faker": "^5.5.3",
    "html-webpack-plugin": "^5.3.2",
    "jsdom": "16.5.0",
    "json-schema-faker": "0.5.0-rc16",
    "json-server": "^0.17.2",
    "localtunnel": "^2.0.2",
    "mini-css-extract-plugin": "^1.6.1",
    "mocha": "^9.2.2",
    "npm-run-all": "4.1.5",
    "numeral": "2.0.6",
    "open": "6.4.0",
    "rimraf": "2.6.2",
    "serve-favicon": "2.5.0",
    "style-loader": "0.23.1",
    "webpack": "^5.76.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.3"
  }
}
