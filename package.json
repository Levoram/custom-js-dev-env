{
  "name": "custom-javascript-development-environment",
  "version": "1.0.2",
  "description": "Javascript Development Environment: Setup Development faster",
  "type": "module",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage",
    "start": "npm-run-all --parallel servesrc start-mockapi",
    "servesrc": "babel-node buildScripts/srcServer",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "npm audit",
    "share": "lt --port 3000",
    "test": "mocha --reporter progress buildScripts/testSetup.cjs \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3005",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "author": "Nico Kuechler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Levoram/custom-js-dev-env"
  },
  "dependencies": {
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@types/webpack": "^5.28.1",
    "babel-loader": "^9.1.2",
    "browserslist": "^4.16.6",
    "caniuse-lite": "^1.0.30001240",
    "chai": "^4.3.4",
    "chalk": "^5.2.0",
    "cheerio": "*",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-watch": "^8.0.0",
    "espree": "^9.5.0",
    "express": "^4.18.2",
    "faker": "^5.5.3",
    "html-webpack-plugin": "^5.3.2",
    "jsdom": "^21.1.1",
    "json-schema-faker": "^0.5.0-rcv.46",
    "json-server": "^0.17.2",
    "localtunnel": "^2.0.2",
    "mini-css-extract-plugin": "^2.7.5",
    "mocha": "^10.2.0",
    "npm-run-all": "4.1.5",
    "numeral": "2.0.6",
    "open": "^9.1.0",
    "rimraf": "5.0.0",
    "serve-favicon": "2.5.0",
    "style-loader": "^3.3.2",
    "url": "^0.11.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.2",
    "webpack-hot-middleware": "^2.25.3"
  },
  "browserslist": "> 0.25%, not dead"
}
